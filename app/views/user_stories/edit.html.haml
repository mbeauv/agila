%h1 UserStories#Edit

= form_for [@project, @user_story] do |f|
  .field
    = f.label :title
    = f.text_field :title

  .field
    = f.label :description
    = f.text_area :description

  .field
    = f.label :estimate
    = f.text_area :estimate

  .actions
    = f.submit

= link_to 'Back', project_user_stories_path(@project)